@article{bok2009,
  title = {Productivity {{Levels}} of {{Rembrandt}} and {{His Main Competitors}} in the {{Amsterdam Art Market}}},
  volume = {51},
  copyright = {Copyright \textcopyright{} 2009 Staatliche Museen zu Berlin -- Preu{\ss}ischer Kulturbesitz},
  issn = {0075-2207},
  abstract = {Rembrandt's production was far from stable. He had periods of high productivity alternating with periods in which he completed only few paintings. We related trends in Rembrandt's productivity to that of three of his main competitors in Amsterdam: Jacob Backer, Ferdinand Bol en Govert Flinck. Because sheer numbers of paintings are of limited value to fully grasp the production of the painters in the context of the highly competitive Amsterdam art market, we also took size and subject matter into consideration. We have observed significant changes in individual productivity during the careers of these four painters. They worked in the same period under the same market conditions, yet their careers differ in many respects. Backer and Bol seem to have had relatively stable levels of production throughout their careers. Their apparently loyal patrons and clients kept faithful to their 'brand'. Backer died at an early age, but Bol became wealthy. Govert Flinck too became wealthy, yet he made a radical decision in the 1650's to concentrate on history paintings. This decision was artistically ambitious yet financially risky. In Flinck's case it turned out well. Rembrandt's career of alternating periods of high and unusually low productivity differs greatly from the other three careers. The inconsistency could be partly due to a lack of personal discipline. Without pressure by employers and creditors he struggled to produce. Another possible explanation might be that the demand for "Rembrandtesque" works may have dwindled in and after the 1640's. Etchings and tuition fees could not sufficiently compensate the loss of income, culminating in Rembrandt's bankruptcy.},
  timestamp = {2015-09-09T18:24:28Z},
  urldate = {2015-09-09},
  url = {http://www.jstor.org/stable/25674337},
  journal = {Jahrbuch der Berliner Museen},
  author = {Bok, Marten Jan and van der Molen, Tom},
  month = jan,
  year = {2009},
  keywords = {Economics,Rembrandt},
  pages = {61--68}
}

