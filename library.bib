@phdthesis{limouze1990,
  address = {Princeton},
  type = {PhD diss.},
  title = {Aegidius {{Sadeler}} (c. 1570-1629): {{Drawings}}, {{Prints}} and {{Art Theory}}},
  copyright = {Copyright UMI - Dissertations Publishing 1990},
  shorttitle = {Aegidius {{Sadeler}} (c. 1570-1629)},
  abstract = {My dissertation examines the career of the Flemish artist, Aegidius Sadeler, who worked in Germany, Italy, and in Prague as court engraver to the Hapsburg emperors Rudolf II, Matthias and Ferdinand II. Sadeler was regarded by Sandrart, Baldinucci and other writers as one of the finest engravers of his time and as a major influence on printmakers of the 17th century.
Sadeler's reputation lies chiefly in his role in the evolution of the reproductive engraving, a genre of print which originated during the 16th century and increased in popularity with the developing interest in art history. However, the dissertation also considers Sadeler's original prints, as well as his less familiar activity as a draftsman and painter, and brings together a group of securely attributed drawings.
While the introductory chapter outlines the state of the literature, the following sections give a revised account of Sadeler's career, and discuss his large oeuvre with regard to stylistic development and subject matter. Chapter 2 deals with the background and religion of the Sadeler family, their emigration to Germany, and Aegidius's activity at the Munich court. Chapter 3 is concerned with the itineraries of Sadeler's two trips to Italy as well as his second stay in Munich. Chapter 4 deals with Sadeler's richest period of activity as one of the distinguished artists brought together by Emperor Rudolf II, while Chapter 5 considers the little-known period in Prague from 1612 to 1629, during which Sadeler remained a powerful artistic influence in Northern Europe. Chapter 6 surveys Sadeler's assistants and pupils, with suggestions regarding their contributions to his work.
The final chapter analyzes Sadeler's prints in relation to early writings on engraving and artistic theory. Particular attention is given the impact of the theory of imitation on his engraving technique, as well as the early regard for reproductive engraving as a valuable medium of instruction by artists and art historians. The dialogue between writings on art and Sadeler's developing engraving style is central to my arguments regarding this artist's importance.},
  language = {English},
  timestamp = {2016-03-23T00:38:29Z},
  urldate = {2016-01-26},
  school = {Princeton University},
  author = {Limouze, Dorothy},
  year = {1990},
  keywords = {Printmaking,Specialization}
}

@phdthesis{lobis2010,
  type = {PhD diss.},
  title = {Artistic {{Training}} and {{Print Culture}} in the {{Time}} of {{Rubens}}},
  copyright = {Copyright ProQuest, UMI Dissertations Publishing 2010},
  abstract = {This dissertation expands and enhances our current knowledge of artistic training in the early modern period by showing how prints were used for the education of artists in seventeenth-century Flanders and, by extension, seventeenth-century Europe. In addition to the history of artistic training, this dissertation addresses fundamental questions about how visual sources were used by early modern artists and about how individual pictorial style was constructed and transmitted.
The narrative arc that I trace begins with Rubens's early training under the lingering structure of the guild system and ends with the reception of Rubens's pictorial style through printed drawing books, whose popularity ascended as the establishment of the institutionalized art academy was cemented. I begin by examining the role of an apprentice's master in the process of learning to draw at the turn of the seventeenth century. Centered on the drawings of Rubens's teachers, the first chapter considers the nature of the instruction that Rubens received. I turn in my second chapter to Rubens's earliest works, pen-drawn copies after prints and illustrated books. These drawings show how Rubens expanded his visual frame of reference and developed his mastery of drawing in the unforgiving medium of pen and ink. It was his early study of prints and printed books, I argue, that set Rubens on a path of life-long engagement with the art of printmaking and the practice of print collecting.
In my third chapter, I propose a reconstruction of Rubens's print collection; again, my methodology depends on evidence found in copy drawings in addition to documentary sources. This reconstruction has implications not only for our understanding of Rubens and his workshop, but for the collection, display, and use of prints by seventeenth-century artists and virtuosi. The final chapter examines Rubens's collaboration with his printmakers, one of whom, I argue, distilled the study collection of the Rubens workshop for more public distribution in the form of a printed drawing book. Unpublished copy drawings after these prints attest to how printed drawing books were studied in late seventeenth-century Flanders, a practice that is markedly different from Rubens's idiosyncratic selection and study of printed models.
Taken together, the circulation of printed models, the formation of print collections, and the rise of the new genre of the printed drawing book demonstrate the centrality of prints for artistic training in the seventeenth century.},
  language = {English},
