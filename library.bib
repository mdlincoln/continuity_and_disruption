@phdthesis{packer2012,
  address = {United States -- New York},
  type = {PhD},
  title = {Imitation and {{Innovation}} in {{Materials}} in {{Early Modern Northern European Art}}: {{Pen Prints}}, {{Pen Drawings}}, and {{Pen Paintings}}, c.1580--c.1670},
  copyright = {Copyright ProQuest, UMI Dissertations Publishing 2012},
  shorttitle = {Imitation and {{Innovation}} in {{Materials}} in {{Early Modern Northern European Art}}},
  abstract = {This dissertation explores the phenomenon of pen imitation traceable from the late sixteenth to the late seventeenth century first in the Southern and subsequently in the Northern Netherlands. Over two dozen artists working in print, drawing, and painting, challenged the boundaries between these media by means of their highly illusionistic manual factures. Chapter 1 examines how, during the latter half of the sixteenth century, specialized engravers began to reproduce virtuosic calligraphic handwriting in print, resulting in works of high similitude in like manner to facsimiles. Conversely, draftsmen working in pen and ink imitated or alluded in varying degrees to different print techniques in meticulously finished stand-alone works. Chapters 2, 3, and 4 address various modes of pen handling that provide a spectrum of referentiality to print. After Jan Wierix began to imitate the delicate linear style of his family's print shop in Flanders, this practice was adopted and refined in the Northern Netherlands by the great printmaker Hendrick Golztius and his followers. By the middle of the seventeenth century, draftsmen like Gerrit and Willem de Heer continued working in this tradition of pen imitation but also adopted certain pictorial strategies from contemporary painting. Chapter 5 details how around 1650, imitative pen drawing was translated to panel and canvas, resulting in pen paintings in the marine genre.
This dissertation is the first scholarly study to consider calligraphic prints, print-like pen drawings, and pen paintings together and in depth, and to argue that these were part of a related, innovative phenomenon of pen imitation traceable over the course of a century in the North. The thesis adds to the current Goltzius-centered literature on imitative pen drawings by showing that many lesser-known artists engaged in comparable practices, thereby revealing that this phenomenon was more widespread and versatile than previously assessed. More broadly, the study seeks to expand our understanding of artworks from this period as products of a single, identifiable medium. Collectively, the objects considered reveal that the boundaries between media were purposefully kept fluid and that a single artwork could encompass or evoke several material practices.},
  language = {English},
  timestamp = {2016-03-23T00:39:26Z},
  urldate = {2014-10-12},
  url = {http://search.proquest.com.proxy-um.researchport.umd.edu/docview/1266231880/33AE8965E6CB4B63PQ/4?accountid=14696},
  school = {New York University},
  author = {Packer, Lelia},
  year = {2012}
}

@phdthesis{wyckoff1998,
  address = {New York},
  type = {PhD diss.},
  title = {Innovation and {{Popularization}}: {{Printmaking}} and {{Print Publishing}} in {{Haarlem During}} the 1620s},
  copyright = {Copyright UMI - Dissertations Publishing 1998},
  shorttitle = {Innovation and {{Popularization}}},
  abstract = {This dissertation surveys the work and lives of four printmakers and one print publisher, all active in Haarlem in the 1620s: Willem Akersloot, Cornelis van Kittensteyn, Cornelis Koning, Gillis van Scheyndel and Jan Pietersz Berendrecht. It examines the success of their endeavors in the print trade, as well as their failure to remain in that field in the long run. Their prints, which have been little studied, are diverse, covering a wide range of image types from landscape and costume to portraiture and current events. These images participate in many of the established traditions of printed imagery, while they also helped to define the evolving genres of seventeenth-century Dutch art.
Chapter one examines the fertile conditions that made Haarlem a magnet for artists in the early seventeenth century. Connections are made between the legend of the invention of the printing press in Haarlem, the predominance of printmaking and the origins of Dutch landscape art there. Chapter two describes the printed output of the printmakers, starting with a discussion of so-called genre depictions, which are often overlooked in the intensity of the focus on landscape in Haarlem.
Jan Pietersz Berendrecht's life and his activities in the art and print trade are the subject of chapter three. He played a critical role in the etching career of Esaias van de Velde, and hence in the history of Dutch landscape art. A printer, not a printmaker and a founding member of the book trades guild established in Haarlem in 1616, he only published prints until about 1630, after which he gradually sold off his plates to other publishers and moved to The Hague, where he had already been doing business for many years. In chapter four the lives of the four printmakers in question are examined, nearly all of whom came from upwardly mobile families. They started their careers around 1620, but the information presented here reveals that they too stopped making prints around 1630, one by one moving on to other careers.},
  language = {English},
  timestamp = {2016-03-23T00:39:32Z},
  urldate = {2014-10-12},
  school = {Columbia University},
  author = {Wyckoff, Elizabeth Ann},
  year = {1998}
}

@phdthesis{baines2011,
  address = {Delaware},
  type = {PhD diss.},
  title = {Nicolaes de {{Bruyn}} (1571--1656) and the {{Art}} of the {{Professional Engraver}}},
  copyright = {Copyright ProQuest, UMI Dissertations Publishing 2011},
  abstract = {Nicolaes de Bruyn (1571-1656) produced nearly three hundred prints in the fifty-eight years between his first dated engraving of 1592 to his final dated work of 1650. He had begun his training in Antwerp, where he was born, and was active in that city until 1617, when he moved from the southern, Catholic Netherlands to the northern, Protestant city of Rotterdam. Early in his career, de Bruyn produced small-scale, mostly serial works after his own designs and after the leading print designer of the day, Marten de Vos. After 1600, de Bruyn spent the next five decades honing his specialty as he produced startlingly large-scale engravings depicting forest landscapes. He initially engraved prints after some of the most famous landscape painters of the day, Gillis van Coninxloo and David Vinckboons. By around 1607, however, he had turned away from copying other artists' compositions to designing his own engravings. Furthermore, aside from a few early mythological and genre scenes, the vast majority of de Bruyn's large engravings after about 1600 depicted religious narratives.
De Bruyn has been neglected until this point largely because his life and work often fall between categories established by scholars for studying the history of printmaking. He worked as both a reproductive engraver and an independent designer-engraver at the same time. Because of his work after other artists, de Bruyn has been branded a reproductive engraver, and he appears almost exclusively in scholarship that deals with the work of the painters or print designers of his reproductive engravings. Furthermore, because he made both large-scale, single-leaf landscapes and small-scale, serial prints, reconciling his early and late work presents a challenge. Finally, de Bruyn worked as a professional engraver during a time when our attention has focused on painter-printmakers in general, and on painter-etchers in particular. To his contemporaries and on the market, however, de Bruyn was a printmaker with a distinctive niche, with little competition. Few printmakers after the 1620s regularly made large-scale landscape prints and none did so in such a sustained manner as de Bruyn.},
  language = {English},
  timestamp = {2016-03-23T00:39:38Z},
  urldate = {2014-10-12},
  school = {University of Delaware},
  author = {Baines, Lorena A.},
  year = {2011},
  keywords = {Prints}
}

